RewriteEngine On
RewriteRule ^([a-zA-Z0-9々〆〤ァ-ヴーぁ-ゔa-zα-ωá-źа-яա-ֆა-ჰא-תぁ-ゔァ-ヴー/チャディ様]+)$ clave.php?u=$1
